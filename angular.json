    "web-user": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/web-user",
      "sourceRoot": "apps/web-user/src",
      "prefix": "web-user",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web-user",
            "index": "apps/web-user/src/index.html",
            "main": "apps/web-user/src/main.ts",
            "polyfills": "apps/web-user/src/polyfills.ts",
            "tsConfig": "apps/web-user/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/web-user/src/favicon.ico",
              "apps/web-user/src/assets"
            ],
            "styles": [
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "apps/web-user/src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/web-user/src/assets/sass",
                "libs/web-user/templates/temp000001/src/lib/assets/sass",
                "libs/web-user/templates/temp-covid000001/src/lib/assets/sass"
              ]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web-user/src/environments/environment.ts",
                  "with": "apps/web-user/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/web-user/src/environments/environment.ts",
                  "with": "apps/web-user/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "apps/web-user/src/environments/environment.ts",
                  "with": "apps/web-user/src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-user:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-user:build:production"
            },
            "development": {
              "browserTarget": "web-user:build:development"
            },
            "staging": {
              "browserTarget": "web-user:build:staging"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-user:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web-user/tsconfig.app.json",
              "apps/web-user/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/web-user/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web-user/jest.config.js",
            "tsConfig": "apps/web-user/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/web-user/src/test-setup.ts"
          }
        }
      }
    },
